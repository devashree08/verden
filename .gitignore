IF @ScopeNorm = 'No'
BEGIN
  SET @MaturityProjectionNorm = NULL;
  SET @ProfileTypeNorm = NULL;
END
ELSE IF @ScopeNorm = 'Shared'
BEGIN
  SET @ProfileTypeNorm = 'Shared';
END
ELSE IF @ScopeNorm = 'Yes'
BEGIN
  -- When in-scope, Shared profile is not allowed
  IF @ProfileTypeNorm = 'Shared'
    THROW 50004, 'Invalid ProfileType for Scope=Yes. Allowed: Provider/Consumer.', 1;
END
