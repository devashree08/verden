WHEN @S = 'INHERIT VCS'            THEN 'Inherit VCS'
          WHEN @S = 'INHERIT NETWORK'        THEN 'Inherit Network'
          WHEN @S = 'INHERIT VGS-T'          THEN 'Inherit VGS-T'
          WHEN @S = 'INHERIT TPD'            THEN 'Inherit TPD'
          ELSE NULL
        END;
      IF @ScopeNorm IS NULL
        THROW 50001, 'Invalid Scope. Allowed: Yes/No/Inherit VCS/Inherit Network/Inherit VGS-T/Inherit TPD.', 1;
    END
