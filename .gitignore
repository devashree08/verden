res = subprocess.run(
    ["gemini", "-m", model, "-p", json.dumps(prompt), "--output-format", "json"],
    check=True,
    capture_output=True,
    text=True,
    timeout=90,
)
text = res.stdout.strip()
data = json.loads(text)  # raises if not valid JSON
out = {
    c["name"]: c.get("description", "").strip()
    for c in data.get("columns", [])
    if "name" in c
}
return {k: v for k, v in out.items() if v}
