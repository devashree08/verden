APP_LOG_LEVEL=INFO
DB_PRIMARY=mssql

# OIDC
OIDC_ISSUER_URI=https://keycloak.example.com/realms/yourrealm
OIDC_JWKS_URI=https://keycloak.example.com/realms/yourrealm/protocol/openid-connect/certs
OIDC_ALLOWED_AUDIENCE=mcp-service
