APP_BACKEND=mssql
RESULT_ROW_LIMIT_DEFAULT=100
LOG_LEVEL=INFO
EID_HEADER_NAME=X-User-EID

# Dev path (HTTP): youâ€™ll pass X-User-EID from Postman/Inspector, so DEV_EID can be blank.
# For MCP stdio tests (local), set DEV_EID=<yourEID>.
DEV_EID=

# Prototype auth (HTTP MCP)
REQUIRE_API_KEY=true
API_KEY_HEADER_NAME=X-API-Key
API_KEYS_JSON_SECRET=           # prefer Secret Manager in Cloud Run
API_KEYS_JSON=                  # OR local-only: {"keys":[{"label":"dev","key":"REPLACE_ME"}]}

# MSSQL (if using on-prem DB locally)
DB_SERVER=REPLACE_ME
DB_DATABASE=REPLACE_ME
DB_USERNAME=REPLACE_ME
DB_PASSWORD=REPLACE_ME
DB_ENCRYPT=false
DB_TRUST_SERVER_CERT=true
DB_CONN_TIMEOUT_SEC=5
DB_COMMAND_TIMEOUT_SEC=30
DB_REQUEST_TIMEOUT_SEC=30
DB_MAX_CONNECTIONS=5

# BigQuery (phase 2.5 if you flip APP_BACKEND=bigquery)
BQ_PROJECT=REPLACE_ME
BQ_LOCATION=US
BQ_DATASET=REPLACE_ME
BQ_TABLE_VALUES=apps_values_current
BQ_TABLE_SUMMARY=apps_values_monthly
BQ_TABLE_GENERAL=apps_vast_general
BQ_TABLE_EID_MAP=sec_eid_vast_map
# GOOGLE_APPLICATION_CREDENTIALS=

# MCP HTTP (local run)
MCP_HTTP_HOST=0.0.0.0
MCP_HTTP_PORT=8081

GLOBAL_RATE_LIMIT_RPS=50
MAX_REQUEST_BODY_BYTES=1048576

# Ops-only
GCP_PROJECT=REPLACE_ME
ARTIFACT_REGISTRY_REPO=us-docker.pkg.dev/REPLACE_ME/mcp
